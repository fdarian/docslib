{
	"compilerOptions": {
		"target": "ESNext",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"paths": {
			"#/*": ["./src/*"]
		}
	},
	"include": ["src", "entries"]
}
